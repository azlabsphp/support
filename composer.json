{
    "name": "drewlabs/support",
    "description": "Utility library for creating common objects like value object, collection, action creator etc...",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Support\\": "src/"
        },
        "files": [
            "./src/proxies.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Support\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2|^8.0",
        "drewlabs/core-helpers": "^0.3",
        "drewlabs/contracts": "^0.3",
        "symfony/polyfill-php81": "^1.24",
        "psr/container": "^1.0|^2.0",
        "drewlabs/collections": "^0.3"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
